<templateSet group="JavaScript">
  <template name="for" value="for (var $INDEX$ = 0, l = $ARRAY$.length; $INDEX$ &lt; l; $INDEX$++)&#10;{&#10;    var $VAR$ = $ARRAY$[$INDEX$];&#10;    $END$&#10;}" description="for loop over array" toReformat="false" toShortenFQNames="true">
    <variable name="INDEX" expression="" defaultValue="&quot;i&quot;" alwaysStopAt="true" />
    <variable name="ARRAY" expression="" defaultValue="&quot;items&quot;" alwaysStopAt="true" />
    <variable name="VAR" expression="" defaultValue="&quot;item&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="fn" value="function $FUNCTION_NAME$($ARGUMENTS$)&#10;{&#10;    console.log('$FUNCTION_NAME$', arguments);&#10;}" description="create function definition" toReformat="false" toShortenFQNames="true">
    <variable name="FUNCTION_NAME" expression="" defaultValue="&quot;functionName&quot;" alwaysStopAt="true" />
    <variable name="ARGUMENTS" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="circ" value="new Path.Circle({&#10;    center: view.center,&#10;    radius: 50,&#10;    fillColor: 'orange'&#10;})" description="draw a circle" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="text" value="new PointText({&#10;    content: '$MESSAGE$',&#10;    point: view.center + [0, -80],&#10;    justification: 'center'&#10;});" description="Draw a PointText" toReformat="false" toShortenFQNames="true">
    <variable name="MESSAGE" expression="" defaultValue="&quot;Say something&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="cl" value="console.log($CONTENT$);" description="Console.log()" toReformat="false" toShortenFQNames="true">
    <variable name="CONTENT" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="p" value="new Point($X$,$Y$)" description="new Point()" toReformat="false" toShortenFQNames="true">
    <variable name="X" expression="" defaultValue="&quot;0&quot;" alwaysStopAt="true" />
    <variable name="Y" expression="" defaultValue="&quot;0&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="center" value="project.activeLayer.position = view.center;" description="position layer at view center" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="panel" value="paperjsLayersPanel.create({items:{expanded:true}});" description="instantiate paperjs layers panel" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="scale" value="project.activeLayer.fitBounds(view.bounds.scale(0.8));" description="scale active layer to view bounds" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="arrow" value="$PARAMETERS$ =&gt; $END$" description="arrow function" toReformat="true" toShortenFQNames="true">
    <variable name="PARAMETERS" expression="" defaultValue="&quot;()&quot;" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>