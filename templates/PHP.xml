<templateSet group="PHP">
  <template name="debug" value="// debug&#10;echo '$VAR$ =';&#10;var_dump($VAR$);&#10;" description="print vars" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="iss" value="isset($$$VAR$)" description="isset(...)" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="&quot;var&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="fun" value="$VISIBILITY$ function $FUNC_NAME$($ARG$)&#10;{&#10;    $END$&#10;}" description="class method" toReformat="false" toShortenFQNames="true">
    <variable name="VISIBILITY" expression="" defaultValue="&quot;public&quot;" alwaysStopAt="true" />
    <variable name="FUNC_NAME" expression="" defaultValue="&quot;functionName&quot;" alwaysStopAt="true" />
    <variable name="ARG" expression="" defaultValue="&quot;$arg&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="kv" value="'$KEY$' =&gt; $VALUE$" description="key =&gt; value" toReformat="false" toShortenFQNames="true">
    <variable name="KEY" expression="" defaultValue="&quot;key&quot;" alwaysStopAt="true" />
    <variable name="VALUE" expression="" defaultValue="&quot;value&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="ion" value="isset($VAR$) ? $VAR$ : $NULL$;" description="isset or null" toReformat="false" toShortenFQNames="true">
    <variable name="VAR" expression="" defaultValue="&quot;$var&quot;" alwaysStopAt="true" />
    <variable name="NULL" expression="" defaultValue="&quot;null&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="abs" value="abstract public function $NAME$($ARG$);" description="abstract function" toReformat="false" toShortenFQNames="true">
    <variable name="NAME" expression="" defaultValue="&quot;functionName&quot;" alwaysStopAt="true" />
    <variable name="ARG" expression="" defaultValue="&quot;$arg1&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP" value="true" />
    </context>
  </template>
  <template name="thr" value="throw new \Exception('$MSG$');" description="throw new" toReformat="true" toShortenFQNames="true">
    <variable name="MSG" expression="" defaultValue="&quot;message&quot;" alwaysStopAt="true" />
    <context>
      <option name="PHP Statement" value="true" />
    </context>
  </template>
  <template name="locale wrap" value="'._('$SELECTION$').'" description="wrap selection with locale function call" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="PHP String Literal" value="true" />
    </context>
  </template>
</templateSet>